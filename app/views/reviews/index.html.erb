<h1>Listing reviews</h1>

<table>
  <tr>
    <th>Day</th>
    <th>Time</th>
    <th>Style</th>
    <th>Difficulty</th>
    <th>Meditative guidance</th>
    <th>Personal touch</th>
    <th>Thoughts</th>
    <th>Teacher</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @reviews.each do |review| %>
  <tr>
    <td><%= review.day %></td>
    <td><%= review.time %></td>
    <td><%= review.style %></td>
    <td><%= review.difficulty %></td>
    <td><%= review.meditative_guidance %></td>
    <td><%= review.personal_touch %></td>
    <td><%= review.thoughts %></td>
    <td><%= review.teacher_id %></td>
    <td><%= review.user_id %></td>
    <td><%= link_to 'Show', review %></td>
    <td><%= link_to 'Edit', edit_review_path(review) %></td>
    <td><%= link_to 'Destroy', review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Review', new_review_path %>
