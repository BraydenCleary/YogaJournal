<h1>Search Results:</h1>


<ul>
<% @response["businesses"].each do |business| %>
	<li style='font-weight: bold;'><%= business['name'] %></li>
	<ul>
		<li>Rating: <%= business['rating'] %></li>
		<li>Link to <%= link_to 'Yelp Page', "http://www.yelp.com/biz/ojas-wellness-center-baltimore" %></li>
		<li>Phone Number: <%= business['display_phone'].gsub('+','').slice(2..13) %></li>
	</ul>
<% end %>
</ul>